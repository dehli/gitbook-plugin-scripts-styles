{
    "name": "gitbook-plugin-scripts-styles",
    "version": "0.0.1",
    "main": "index.js",
    "description": "Add scripts and styles to your gitbook pages",
    "engines": {
        "gitbook": ">1.x.x"
    },
    "dependencies": {
        "tmp": "0.0.28",
        "lodash": "*"
    },
    "author": "Erez Odier <erez.odier@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mwerezi/gitbook-plugin-scripts-styles.git"
    },
    "bugs": {
        "url": "https://github.com/mwerezi/gitbook-plugin-scripts-styles/issues"
    },
    "gitbook": {
        "properties": {
            "scripts-styles": {
                "type": "object",
                "title": "custom scripts and styles",
                "properties": {
                    "js": {
                        "type": "array"
                    },
                    "css": {
                        "type": "array"
                    }
                }
            }
        }
    }
}